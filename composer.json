{
    "name": "rebelcode/wp-settings-interface",
    "description": "Interfaces for an object oriented API for WordPress settings.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.3 | ^7.0",
        "dhii/data-key-value-aware-interface": "^0.1",
        "dhii/human-readable-interface": "dev-task/initial-interfaces",
        "dhii/type-interface": "dev-task/initial-interfaces",
        "dhii/output-renderer-interface": "dev-release/0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "dev-php-5.3",
        "codeclimate/php-test-reporter": "<=0.3.2"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\WordPress\\Admin\\Settings\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/output-renderer-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/output-block-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/human-readable-interface"
        }
    ]
}
